#include <iostream>
using namespace std;

int main() 
{
    int a;
    int b;
    int c;
    
    cout << " Put any three numbers: ";
    cin >> a;
    cin >> b;
    cin >> c;
    
    /*logic*/
    
    if( a>=b && a>=c){
      
      cout << "The largest number is: " << a << endl;
    }
    if( b>=a && b>=c){
      
      cout << "The largest number is: " << b << endl;
    }
    if( c>=a && c>=b){
      
      cout << "The largest number is: " << c <<endl;
    }
    return 0;
}
